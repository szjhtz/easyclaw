# WeCom (WeChat Work) credentials
WECOM_CORPID=your_corp_id
# Use the Customer Service secret (微信客服 → 企业内部接入 → Secret), NOT the general app secret
WECOM_APP_SECRET=your_customer_service_secret
WECOM_TOKEN=your_wecom_callback_token
WECOM_ENCODING_AES_KEY=your_43_character_encoding_aes_key_here_abc
# Get via API: GET /cgi-bin/kf/account/list — starts with "wk", NOT the "kfc" ID shown in admin console
WECOM_OPEN_KFID=wkXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
# The "kfc" ID from admin console (微信客服 → 客服账号) — used for customer-facing link URL
WECOM_KF_LINK_ID=kfcXXXXXXXXXXXXXXXXXX

# Server ports
PORT=3000
WS_PORT=3001

# Shared secret for gateway WebSocket authentication
RELAY_AUTH_SECRET=your_relay_auth_secret

# SQLite database path
DATABASE_PATH=./data/relay.db
